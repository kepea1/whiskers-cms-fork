---
import Layout from "./layout.astro";

import Home from "./home.astro";
import KeyPoints from "src/pages/key_Points.astro";
import Gallery from "./gallery.astro";

import Testimonials from "src/pages/testermonials.astro";
import Services_prices from "@/pages/services_Prices.astro";

import "@/styles/global.css";
---

<Layout title="Home Page">
  <section id="home" class="pt-8 md:pt-16">
    <Home />
  </section>
  <section id="services" class="mt-18 md:pt-28">
    <Services_prices />
  </section>
  <section id="services" class="mt-18 md:pt-28">
    <KeyPoints />
  </section>
  <section id="testimonials" class="mt-18 md:pt-28">
    <Testimonials />
  </section>
  <section id="gallery" class="mt-18 md:pt-28">
    <Gallery />
  </section>
</Layout>
